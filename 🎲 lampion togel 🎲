<script>
$(document).ready(function() {
  var clearCounter = localStorage.getItem('clearCounter');
  if (!clearCounter) {
    clearCacheAndStorage();
    localStorage.setItem('clearCounter', '1');
  }
});

function clearCacheAndStorage() {
  window.location.reload(true);
  localStorage.clear();
}
</script>
