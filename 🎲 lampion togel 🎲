<script type="text/javascript">
  var cacheData;
  var dataString = {};
  dataString["per"]="1";
  dataString["sar"]="2";

  var data = $('loadbalance').html();

  function loadbalance(){
      $('#putar2').addClass('fa-spin');
      $("#putar2").addClass("disabled");
        $.ajax({
            url: 'loadbalance.php',
            type: 'POST',
            data: dataString,
            dataType: 'html',
            success: function(data) {
                if (data !== cacheData){
                    cacheData = data;
                    $('#refreshbalance').html(data);
                }
            }
        })
        setTimeout(function(){ $("#putar2").removeClass('disabled'); }, 10000);
        setTimeout(function(){ $('#putar2').removeClass('fa-spin'); }, 1500);
  }
</script>
